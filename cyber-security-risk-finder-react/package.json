  "description": "En este repositorio encontrarás el código fuente de una pequeña aplicación que tiene como propósito facilitar la búsqueda de riesgos de ciberseguridad",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "json-server": "json-server --config src/utils/json-server/config.json --watch src/utils/json-server/db.json --fks _id",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:run": "vitest --run --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:watch": "vitest --watch --config vitest.config.ts"
  },
